{
  "name": "candidates",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.hostname": "postgres",
    "database.port": "5432",
    "database.password": "postgres",
    "database.user": "postgres",
    "database.dbname": "postgres",
    "database.server.name": "postgres",
    "decimal.handling.mode": "double",
    "table.include.list": "public.users,public.experiences",
    "plugin.name": "pgoutput",
    "snapshot.mode": "always",
    "publication.autocreate.mode": "filtered",
    "transforms": "ReroutePublic,unwrap",
    "snapshot.delay.ms": 10000,
    "topic.creation.enable": true,
    "topic.creation.default.replication.factor": 1,
    "errors.retry.timeout": 300000,
    "errors.retry.delay.max.ms": 60000,
    "errors.log.enable": true,
    "topic.creation.default.partitions": 10,
    "topic.creation.default.cleanup.policy": "compact",
    "topic.creation.default.max.compaction.lag.ms": "60000",
    "topic.creation.default.compression.type": "lz4",
    "transforms.SetSchemaMetadata.type": "org.apache.kafka.connect.transforms.SetSchemaMetadata$Value",
    "transforms.SetSchemaMetadata.schema.name": "${topic}",
    "transforms.ReroutePublic.type": "io.debezium.transforms.ByLogicalTableRouter",
    "transforms.ReroutePublic.topic.regex": "postgres.public(.*)",
    "transforms.ReroutePublic.topic.replacement": "candidates.tables.main$1",
    "key.enforce.uniqueness": false,
    "max.request.size": 50485760,
    "producer.override.max.request.size": 50485760,
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite"
  }
}
